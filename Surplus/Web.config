    <!-- CAS Configuration -->
    <add key="enableSimpleMembership" value="false" />
    <add key="autoFormsAuthentication" value="false" />
    <!-- /CAS Configuration -->

  </appSettings>

  <connectionStrings>
    <add name="SurplusDbContext"
         connectionString="Server=(localdb)\v11.0;Database=surplus_db4;Integrated Security=true"
         providerName="System.Data.SqlClient" />
  </connectionStrings>

  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />

    <!-- CAS Configuration -->
    <authentication mode="Forms">
      <!--<forms loginUrl="~/Account/Login" timeout="2880" />-->
      <forms loginUrl="https://auth.vt.edu" cookieless="UseCookies" />
    </authentication>
    <roleManager defaultProvider="CasRoleProvider" enabled="true" cacheRolesInCookie="true">
      <providers>
        <clear />
        <add name="CasRoleProvider" type="Surplus.DataAccess.CasRoleProvider" roleAttributeName="virginiaTechAffiliation" />
      </providers>
    </roleManager>
    <!-- /CAS Configuration -->

  </system.web>
  <runtime>
